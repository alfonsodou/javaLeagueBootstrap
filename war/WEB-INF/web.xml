		<servlet-class>org.javahispano.javaleague.server.UploadBlobstoreServiceImpl</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>uploadBlobstoreService</servlet-name>
		<url-pattern>/javaleague/uploadBlobstoreService</url-pattern>
	</servlet-mapping>

