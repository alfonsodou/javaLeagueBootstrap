	<servlet>
		<servlet-name>matchService</servlet-name>
		<servlet-class>org.javahispano.javaleague.server.MatchServiceImpl
		</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>matchService</servlet-name>
		<url-pattern>/javaleague/matchService</url-pattern>
	</servlet-mapping>

	<!-- ===================== Login Google Servlet ===================== -->

	<servlet>
		<servlet-name>logingoogle</servlet-name>
		<servlet-class>org.javahispano.javaleague.server.servlets.LoginGoogleServlet
		</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>logingoogle</servlet-name>
		<url-pattern>/logingoogle</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>logingooglecallback</servlet-name>
		<servlet-class>org.javahispano.javaleague.server.servlets.LoginGoogleCallbackServlet
		</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>logingooglecallback</servlet-name>
		<url-pattern>/logingooglecallback</url-pattern>
	</servlet-mapping>

	<!-- ===================== Login Twitter Servlet ===================== -->

	<servlet>
		<servlet-name>logintwitter</servlet-name>
		<servlet-class>org.javahispano.javaleague.server.servlets.LoginTwitterServlet
		</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>logintwitter</servlet-name>
		<url-pattern>/logintwitter</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>logintwittercallback</servlet-name>
		<servlet-class>org.javahispano.javaleague.server.servlets.LoginTwitterCallbackServlet
		</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>logintwittercallback</servlet-name>
		<url-pattern>/logintwittercallback</url-pattern>
	</servlet-mapping>

	<!-- ===================== Login Facebook Servlet ===================== -->

	<servlet>
		<servlet-name>loginfacebook</servlet-name>
		<servlet-class>org.javahispano.javaleague.server.servlets.LoginFacebookServlet
		</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>loginfacebook</servlet-name>
		<url-pattern>/loginfacebook</url-pattern>
	</servlet-mapping>

	<!-- ===================== UserFile Service ===================== -->
	<servlet>
		<servlet-name>userFileService</servlet-name>
		<servlet-class>org.javahispano.javaleague.server.UserFileServiceImpl
		</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>userFileService</servlet-name>
		<url-pattern>/javaleague/userFileService</url-pattern>
	</servlet-mapping>

	<!-- ===================== Upload Servlet ===================== -->
	<servlet>
		<servlet-name>upload</servlet-name>
		<servlet-class>org.javahispano.javaleague.server.servlets.UploadServlet
		</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>upload</servlet-name>
		<url-pattern>/upload</url-pattern>
	</servlet-mapping>

	<!-- ===================== Dispatch Match Servlet ===================== -->
	<servlet>
		<servlet-name>dispatchMatch</servlet-name>
		<servlet-class>org.javahispano.javaleague.server.servlets.DispatchMatchServlet
		</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>dispatchMatch</servlet-name>
		<url-pattern>/dispatchMatch</url-pattern>
	</servlet-mapping>

	<!-- ===================== Play Match Servlet ===================== -->
	<servlet>
		<servlet-name>playMatch</servlet-name>
		<servlet-class>org.javahispano.javaleague.server.servlets.PlayMatchServlet
		</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>playMatch</servlet-name>
		<url-pattern>/playMatch</url-pattern>
	</servlet-mapping>

	<!-- ===================== Serve Match Servlet ===================== -->
	<servlet>
		<servlet-name>Serve</servlet-name>
		<servlet-class>org.javahispano.javaleague.server.servlets.ServeServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Serve</servlet-name>
		<url-pattern>/serve</url-pattern>
	</servlet-mapping>
	
	<!-- ===================== Daytime  Servlet ===================== -->
	<servlet>
		<servlet-name>Daytime</servlet-name>
		<servlet-class>org.javahispano.javaleague.server.servlets.DaytimeServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Daytime</servlet-name>
		<url-pattern>/daytime</url-pattern>
	</servlet-mapping>
	
	<!-- Default page to serve -->
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>



	<servlet>
		<servlet-name>SystemServiceServlet</servlet-name>
		<servlet-class>com.google.api.server.spi.SystemServiceServlet
		</servlet-class>
		<init-param>
			<param-name>services</param-name>
			<param-value />
		</init-param>
	</servlet>

	<servlet-mapping>
		<servlet-name>SystemServiceServlet</servlet-name>
		<url-pattern>/_ah/spi/*</url-pattern>
	</servlet-mapping>

</web-app>