	</servlet>
	<servlet-mapping>
		<servlet-name>playMatchFriendly</servlet-name>
		<url-pattern>/playMatchFriendly</url-pattern>
	</servlet-mapping>

	<!-- ===================== Serve Match Servlet ===================== -->
	<servlet>
		<servlet-name>Serve</servlet-name>
		<servlet-class>org.javahispano.javaleague.server.servlets.ServeServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Serve</servlet-name>
		<url-pattern>/serve</url-pattern>
	</servlet-mapping>

	<!-- ===================== Serve Bin Match Servlet ===================== -->
	<servlet>
