		<servlet-name>tacticService</servlet-name>
		<servlet-class>org.javahispano.javaleague.server.TacticServiceImpl
		</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>tacticService</servlet-name>
		<url-pattern>/javaleague/tacticService</url-pattern>
	</servlet-mapping>

