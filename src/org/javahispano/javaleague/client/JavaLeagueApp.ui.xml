<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:app="urn:import:org.javahispano.javaleague.client"
	xmlns:b="urn:import:com.github.gwtbootstrap.client.ui"
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>

	<g:HTMLPanel>
		<app:MyHeaderPanel ui:field='myHeaderPanel'></app:MyHeaderPanel>
		<app:MyCenterPanel ui:field='myCenterPanel'></app:MyCenterPanel>
		<app:MyFooterPanel ui:field='myFooterPanel'></app:MyFooterPanel>
	</g:HTMLPanel>
</ui:UiBinder> 