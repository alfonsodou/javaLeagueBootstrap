<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:app="urn:import:org.javahispano.javaleague.client"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui" ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>

	<ui:style>
        .buttons button {
            margin: 0.5em;
        }

        .labels span {
            margin-right: 5px;
        }

        .push {
            padding-top: 70px;
        }
	</ui:style>

	<g:ScrollPanel>
		<b:Container>
			<g:SimplePanel ui:field="headerPanel"></g:SimplePanel>

			<g:SimplePanel ui:field="centerPanel" addStyleNames="{style.push}">
			</g:SimplePanel>

			<g:SimplePanel ui:field="footerPanel">

				<b:Paragraph ui:field="footerPanelParagraph">
					2014 -
					<a href="http://www.javahispano.org">javaHispano</a>
					::
					<a href="https://github.com/svenjacobs/gwtbootstrap3">GwtBootstrap3</a>
				</b:Paragraph>

			</g:SimplePanel>
		</b:Container>
	</g:ScrollPanel>

</ui:UiBinder>