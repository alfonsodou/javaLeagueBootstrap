<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' xmlns:b='urn:import:com.svenjacobs.gwtbootstrap3.client.ui'
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>

	<ui:style>
		.danger {
			background-color: #fcf2f2;
			border-color: #dFb5b4;
		}
		
		.margin-fix {
			margin-bottom: 0 !important;
		}
	</ui:style>

	<b:Navbar>

		<b:NavbarHeader>
			<b:NavbarBrand>javaLeague</b:NavbarBrand>
			<b:NavbarCollapseButton target="#navbar-collapse2" />
		</b:NavbarHeader>

		<b:NavbarCollapse>
			<b:Nav>
				<b:NavbarLink ui:field="myTeamLink">
					<ui:msg key="myTeamLink">Mi equipo</ui:msg>
				</b:NavbarLink>
				<b:NavbarLink ui:field="myLeaguesLink">
					<ui:msg key="myLeaguesLink">Mis ligas</ui:msg>
				</b:NavbarLink>
			</b:Nav>
			<b:Nav alignment="RIGHT">
				<b:NavbarLink ui:field="logoutLink">
					<ui:msg key="logoutLink">Salir</ui:msg>
				</b:NavbarLink>

				<b:ListDropDown text="Español (es)" icon="GLOBE">
					<b:ListItem>Español (es)</b:ListItem>
					<b:ListItem>English (en)</b:ListItem>
				</b:ListDropDown>
			</b:Nav>
		</b:NavbarCollapse>
	</b:Navbar>




</ui:UiBinder> 