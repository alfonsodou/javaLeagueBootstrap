<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' xmlns:b='urn:import:org.gwtbootstrap3.client.ui'
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>

	<ui:style>
		.danger {
			background-color: #fcf2f2;
			border-color: #dFb5b4;
		}
		
		.margin-fix {
			margin-bottom: 0 !important;
		}
	</ui:style>

	<b:Navbar position="FIXED_TOP">
		<b:Container>
			<b:NavbarHeader>
				<b:NavbarBrand>javaLeague</b:NavbarBrand>
				<b:NavbarCollapseButton target="#navbar-collapse" />
			</b:NavbarHeader>

			<b:NavbarCollapse b:id="#navbar-collapse">
				<b:NavbarNav>
					<b:ListItem ui:field="myTeamLink">
						<ui:msg key="myTeamLink">Mi equipo</ui:msg>
					</b:ListItem>
					<b:ListItem ui:field="myLeaguesLink">
						<ui:msg key="myLeaguesLink">Mis ligas</ui:msg>
					</b:ListItem>

					<b:ListItem ui:field="logoutLink">
						<ui:msg key="logoutLink">Salir</ui:msg>
					</b:ListItem>

					<b:ListDropDown>
						<b:AnchorButton toggle="DROPDOWN" icon="GLOBE">Español
							(es)</b:AnchorButton>
						<b:DropDownMenu>
							<b:ListItem>Español (es)</b:ListItem>
							<b:ListItem>English (en)</b:ListItem>
						</b:DropDownMenu>
					</b:ListDropDown>

				</b:NavbarNav>



			</b:NavbarCollapse>
		</b:Container>
	</b:Navbar>




</ui:UiBinder> 