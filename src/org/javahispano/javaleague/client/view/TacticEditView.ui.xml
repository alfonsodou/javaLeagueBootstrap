<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui" 
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>


	<b:Form type="HORIZONTAL" ui:field="form">
		<b:FieldSet>
			<b:FormGroup>
				<b:FormLabel for="teamName">
					<ui:message key="teamName">Nombre</ui:message>
				</b:FormLabel>

				<!-- If you add id attribute to element,You should use b:id attribute. -->
				<b:TextBox alternateSize="XLARGE" b:id="teamName"
					ui:field="teamName"></b:TextBox>
				<b:TextBox ui:field="tacticId" visible="false"></b:TextBox>
				<b:HelpBlock>
					<ui:message key="helpTeamName">Introduce el nombre de tu equipo.</ui:message>
				</b:HelpBlock>

			</b:FormGroup>
			<b:FormGroup>
				<b:FormLabel for="zipClasses">
					<ui:message key="zipClasses">Clases</ui:message>
				</b:FormLabel>

				<!-- If you add id attribute to element,You should use b:id attribute. -->

				<g:FileUpload b:id="zipClasses" ui:field="fileUpload"></g:FileUpload>

			</b:FormGroup>
			<b:ButtonGroup>
				<b:SubmitButton addStyleNames="btn" ui:field="updateButton">
					<ui:msg key="updateTactic">Actualizar</ui:msg>
				</b:SubmitButton>
				<b:Button ui:field="cancelButton">
					<ui:msg key="cancelTactic">Cancelar</ui:msg>
				</b:Button>
			</b:ButtonGroup>
		</b:FieldSet>
	</b:Form>
